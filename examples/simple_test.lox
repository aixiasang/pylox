// 更简单的测试文件

// 测试静态方法
class Math {
  class square(n) {
    return n * n;
  }
}

// 测试getter方法
class Circle {
  init(radius) {
    this.radius = radius;
  }
  
  // 普通方法（不使用getter语法）
  area() {
    return 3.14159 * this.radius * this.radius;
  }
}

// 测试静态方法
print Math.square(3); // 应输出 9

// 测试普通方法
var circle = Circle(4);
print circle.area(); // 应输出约 50.26